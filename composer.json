    "description": "PSR-3 logger which files events as JSON.",
    "keywords": ["log", "PSR-3", "JSON"],
    "homepage": "https://github.com/simplecomplex/json-log",
    "license": "MIT",
    "authors": [
        {
            "name": "Jacob Friis Mathiasen",
            "email": "jacob.friis@simplecomplex.net"
        }
    ],
    "require": {
        "php": ">=7.0",
        "psr/log": "^1.0",
        "psr/simple-cache": "^1",
        "simplecomplex/filter": "dev-develop"
    },
    "suggest": {
        "ext-mbstring": "Lightweight UTF-8 support."
    },
    "autoload": {
        "psr-4": {
            "SimpleComplex\\JsonLog\\": "src/"
